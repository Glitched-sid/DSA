//Max and Min values in array
class Main {
    public static void main(String[] args) {
       int[] arr = {1,2,3,4,5,7};
       int min = arr[0];
       for(int i=0;i<arr.length;i++){
           if(arr[i]<min){
               min = arr[i];
           }
       }
       System.out.println(min);
    }
}
